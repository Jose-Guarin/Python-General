dic = {'name': 'John', 'age':25, 'courses':['math','compsci']}
print(dic)

#GETTING VALUE
print(dic['name']) # if index does not exist it returns an error
print(dic.get('name')) #if index does not exist, it returns none
print(dic.get('name','Not Found')) # if index does not exist it returns a specific value.

#SETTING OR UPDATING A VALUE
dic('key') = 'value'
dic.update({'name':'Jose','age':26}) # The method allows to update differents keys

#DELETE
del dic['age']
age = dic.pop('age') #get and print the popped value.
print(age)

print(len(dic)) # get the lenght

#GET KEYS/VALUES/KEY+VALUE
print(dic.keys())
print(dic.values())
print(dic.item())  == dic_items([('name','John'),(age,25)])

for key in dic.item():
  print(key)

for key,value in dic.item():
  print(key,value)




  
